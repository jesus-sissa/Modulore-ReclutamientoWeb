                        $(element).attr("disabled", "disabled");
                    });
                    $("#tabGineAntecCard input").each(function(index, element) {
                        $(element).attr("disabled", "disabled");
                    });

                    $("#guardar").attr("disabled", "disabled")
                    $("#guardar").attr("hidden", "hidden")
                } else {
                    $("#guardar").removeAttr("disabled")
                    $("#guardar").removeAttr("hidden")
                }

            });

