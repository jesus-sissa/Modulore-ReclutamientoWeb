        function ConfirmDialog() {
            swal(
                {
                    title: "Confirmar Clave Prospecto!!",
                    text: "Ingresa la clave que te proporcionaron",
                    type: "input",
                    showCancelButton: true,
                    closeOnConfirm: false,
                    animation: "slide-from-top",
                    inputPlaceholder: "Clave",
                },
                function(inputValue) {
                    if (inputValue === false) {

                        return false;

                    }
                    if (inputValue === "") {
                        swal.showInputError("Ingresar Clave!");
                        _nombreBeneficiario = "";
                        _parentesco = "";
                        _porcentaje = 0;
                        return false;
                    }
                    if ('@ViewBag.pto' === inputValue) {
                        ConfirmDocument();
                    } else {
                        swal.showInputError("Clave Erronea!");
                        return false;
                    }

                }
            );
        }

        function ConfirmDocument() {
            var _id = $("#medicalId").val();
            $.ajax({
                type: "POST",
                url: "@Url.Action("ConfirmMedicalQuestionnaireDocument","Prospectus")",
                data: { medical: _id },

                success: function(data) {
                    if (data) {
                        swal("Felicidades!!", "Firmado Correctamente", "success");
                    }
                    else {
                        swal("Oopsss !!", "Parece ocurrio un problema vuelve a intentarlo!!", "error");
                    }

                    setTimeout(function() {
                        window.location.href = '@Url.Action("MedicalQuestionnaire","Prospectus",new{ pto=ViewBag.pto,filter=ViewBag.filter})'

                    }, 2000);


                },
                error: function(error) {
                    console.log(error)
                    alert("Error al firmar")
                    //window.location.href ="/cajerosinteligentesQA/Home/Index"
                },
                complete: function() { }
            });
        }


