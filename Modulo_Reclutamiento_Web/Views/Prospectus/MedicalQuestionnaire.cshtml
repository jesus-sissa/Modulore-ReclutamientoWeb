@using Modulo_Reclutamiento_Web.Models.MedicalQuestionData
@model MedicalQuestion
<div class="page-header">
    <div class="page-title">
        <ol class="breadcrumb" style="float:left">
            @if (ViewBag.stat == "R")
            {
                <li class="breadcrumb-item"><a href="@Url.Action("ListOfMedicalQuestionnarie","Recruiter",new{ pto = ViewBag.pto,filter=ViewBag.filter,stat=ViewBag.stat})"><i class="ti-arrow-left"></i> Regresar</a></li>
            }
            else
            {
                <li class="breadcrumb-item"><a href="@Url.Action("Index","Prospectus",new{ pto = ViewBag.pto})"><i class="ti-arrow-left"></i> Regresar</a></li>
            }

        </ol>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">Reclutador</a></li>
            <li class="breadcrumb-item active">Prospectos</li>
            <li class="breadcrumb-item active">Procesos</li>
            <li class="breadcrumb-item active">Cuestionario Medico</li>
        </ol>
    </div>
</div>

<div class="card">
    <div class="card-title">
        <div class="col-12">
            <img src="../images/encabezado.jpg" width="100%" height="55px" title="encabezado" />

        </div>
    </div>
    <div class="card-body">
        <h4 class="card-title text-center">CUESTIONARIO MEDICO</h4>
        <ul class="nav nav-tabs customtab2" role="tablist">
            <li class="nav-item">
                <a class="nav-link active show" data-toggle="tab" id="tabPerData" href="#PerData" role="tab" aria-selected="true">
                    <span class="hidden-sm-up"><i class="ti-user"></i></span>
                    <span class="hidden-xs-down">Informacion de Candidato</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" id="tabPerMedHist" href="#PerMedHist" role="tab" aria-selected="true">
                    <span class="hidden-sm-up"><i class="ti-heart"></i></span>
                    <span class="hidden-xs-down">Antecedentes Médicos Personales</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#GineAntec" id="tabGineAntec" disabled role="tab" aria-selected="true">
                    <span class="hidden-sm-up"><i class="ti-heart-broken"></i></span>
                    <span class="hidden-xs-down">Conteste Lo siguiente solo si usted es mujer</span>
                </a>
            </li>
        </ul>
        <form method="post" asp-controller="Prospectus" asp-action="SaveMedicalQuestionnaire">

            <div class="tab-content">
                <div class="tab-pane active show" id="PerData" role="tabpanel">
                    <div class="p-20">
                        <partial name="../Prospectus/partialViews/MedicalQuestionnaire/_PersonalMedicalData" model="@Model" />
                    </div>
                </div>
                <div class="tab-pane" id="PerMedHist" role="tabpanel">
                    <div class="p-20">
                        <partial name="../Prospectus/partialViews/MedicalQuestionnaire/_PersonalMedicalHistory" model="@Model" />
                    </div>
                </div>
                <div class="tab-pane" id="GineAntec" role="tabpanel">
                    <div class="p-20">
                        <partial name="../Prospectus/partialViews/MedicalQuestionnaire/_GynecologicalAntecento" model="@Model" />
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-xl-4" style="float:left">
                <div class="form-group">
                    <input type="button" id="firmar" class="btn btn-sissa-blue-light form-control" value="Firmar" />
                </div>
            </div>
            <div class="col-lg-4 col-xl-4" style="float:right">
                <div class="form-group">
                    <input type="submit" id="guardar" class="btn btn-sissa-green-light form-control" value="Guardar" />
                </div>
            </div>

        </form>
    </div>
</div>

@section Scripts
    {
    <script type="text/javascript">
        $(document).ready(function() {



            if ('@ViewBag.msg' === '0') {
                sweetAlert("Oops...", "No se pudo guardar el Cuestionario Medico !!", "error");
            }
            if ('@ViewBag.msg' === '1') {
                swal("Cuestionario Medico Guardado!", "", "success");
            }
            if ('@ViewBag.msg' === '2') {
                swal("Cuestionario Medico Actualizado!", "", "success");
            }

            ///////////////////////////////////////////////////////////////////
            //se oculta botones si el cuestionario ya fue firmado por el prospecto
            if ('@ViewBag.CuestionnaireSigned' === 'True') {
                $("#firmar").attr("disabled", "disabled")
                $("#firmar").attr("hidden", "hidden")
                $("#guardar").attr("disabled", "disabled")
                $("#guardar").attr("hidden", "hidden")
            } else {
                $("#firmar").removeAttr("disabled")
                $("#guardar").removeAttr("disabled")
            }

            /// bloqueo de elementos cuando se entra como reclutador
            if ('@ViewBag.stat' === 'R') {
                $("#Weight").removeAttr("disabled")
                $("#height").removeAttr("disabled")
                $("#personalMedicalHistory_BloodPressure").removeAttr("disabled")
                $("#personalMedicalHistory_Glucose").removeAttr("disabled")
                $("#personalMedicalHistory_Remarks").removeAttr("disabled")
                $("#Antidoping").removeAttr("disabled")
                $("#IMC").removeAttr("disabled")
                $("#MedicalServices").removeAttr("disabled")
                $("#MedicalServiceApproval").removeAttr("disabled")
                $("#Remarks").removeAttr("disabled")


                $("#guardar").removeAttr("disabled")
                $("#guardar").removeAttr("hidden")

