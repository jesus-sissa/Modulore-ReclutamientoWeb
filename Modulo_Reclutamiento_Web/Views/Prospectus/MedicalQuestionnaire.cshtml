                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").removeAttr("disabled")
            } else {
                $(".checkLiverDis").attr("disabled", "disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").attr("disabled", "disabled")
                $(".checkLiverDis").removeAttr("checked")

            }
            //diabetes
            var _DiabetesMell = $("#DiabetesMellitus").val();
            //console.log(_wearGlassesVal);
            if (_DiabetesMell === "S") {
                $(".checkDiabetesMellitus").removeAttr("disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").removeAttr("disabled")
            } else {
