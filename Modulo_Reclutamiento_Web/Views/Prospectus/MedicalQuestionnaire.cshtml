                if ('@ViewBag.stat' === 'R') {
                    $("#guardar").removeAttr("disabled")
                    $("#guardar").removeAttr("hidden")
                }
            });
            //antecedentes medicos
            $("#tabPerMedHist").click(function() {

                if ('@ViewBag.stat' === 'R') {

                    $("#PersonalMedicalHistoryCard select").each(function(index, element) {

                        $(element).attr("disabled", "disabled");
                    });
                    $("#PersonalMedicalHistoryCard input").each(function(index, element) {
                        $(element).attr("disabled", "disabled");
                    });

                    $("#guardar").attr("disabled", "disabled")
                    $("#guardar").attr("hidden", "hidden")
                } else {
                    $("#guardar").removeAttr("disabled")
                    $("#guardar").removeAttr("hidden")
                }

            });
            //antecedentes ginecologicos
            $("#tabGineAntec").click(function() {

