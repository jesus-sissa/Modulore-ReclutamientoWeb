            //console.log(_wearGlassesVal);
            if (_NeurologicalDiseases === "S") {
                $(".checkNeuroDis").removeAttr("disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").removeAttr("disabled")
            } else {
                $(".checkNeuroDis").attr("disabled", "disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").attr("disabled", "disabled")
                $(".checkNeuroDis").removeAttr("checked")

            }
            //enfermedad psicologica
            var _PsychiatricIllnesses = $("#PsychiatricIllnesses").val();
            //console.log(_wearGlassesVal);
            if (_PsychiatricIllnesses === "S") {
                $(".checkPsyIll").removeAttr("disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").removeAttr("disabled")
            } else {
                $(".checkPsyIll").attr("disabled", "disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").attr("disabled", "disabled")
                $(".checkPsyIll").removeAttr("checked")

            }
            //enfermedad muscular
            var _MusculoskeletalDiseases = $("#MusculoskeletalDiseases").val();
            //console.log(_wearGlassesVal);
            if (_MusculoskeletalDiseases === "S") {
                $(".checkMuscDis").removeAttr("disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").removeAttr("disabled")
            } else {
                $(".checkMuscDis").attr("disabled", "disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").attr("disabled", "disabled")
                $(".checkMuscDis").removeAttr("checked")

            }
            //enfermedades infecciosas
            var _InfectiousDiseases = $("#InfectiousDiseases").val();
            //console.log(_wearGlassesVal);
            if (_InfectiousDiseases === "S") {
                $(".checkInfectDis").removeAttr("disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").removeAttr("disabled")
            } else {
                $(".checkInfectDis").attr("disabled", "disabled")
                //$("#personalMedicalHistory_AlteredBloodPressureMedicaments").attr("disabled", "disabled")
                $(".checkInfectDis").removeAttr("checked")

            }
            //ingreso al hospital
            var _AdmissionToHospital = $("#AdmissionToHospital").val();
            //console.log(_wearGlassesVal);
            if (_AdmissionToHospital === "S") {
                $(".checkAdmHosp").removeAttr("disabled")
                //$("#personalMedicalHistory_SurgeriesPerformed").removeAttr("disabled")
            } else {
