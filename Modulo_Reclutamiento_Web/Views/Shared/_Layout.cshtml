    <link href="~/css/lib/toastr/toastr.min.css" rel="stylesheet">
    <link href="~/css/lib/sweetalert/sweetalert.css" rel="stylesheet">
    <link href="~/css/lib/menubar/sidebar.css" rel="stylesheet">
    <link href="~/css/lib/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/lib/helper.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet">
    <link href="~/css/lib/scrollable/scrollable.min.css" rel="stylesheet">
    @*    <link href="~/css/lib/data-table/buttons.bootstrap.min.css" rel="stylesheet">
    <link href="~/css/lib/data-table/buttons.dataTables.min.css" rel="stylesheet">
    <link href="~/css/lib/data-table/dataTables.bootstrap.min.css" rel="stylesheet">*@
 @*   <link href="https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.13.4/b-2.3.6/b-colvis-2.3.6/b-html5-2.3.6/b-print-2.3.6/date-1.4.0/rg-1.3.1/sl-1.6.2/datatables.min.css" rel="stylesheet" />*@


    <style type="text/css">
        .box {
            height: 473px;
        }
    </style>

    @*<script src="js/lib/chart-js/chartjs-init.js"></script>*@

</head>

<body class="pace-done">


    <partial name="_sidebar.cshtml" view-data='@new ViewDataDictionary(ViewData) { { "menus", @User_Persistent_Data.menus } }' />

    <partial name="_header.cshtml" />


    <div class="content-wrap">
        <div class="main">
            <div class="container-fluid">
                <section id="main-content">

                    @RenderBody()

                </section>
            </div>
        </div>
    </div>

    <partial name="_scripts.cshtml" />

    @await RenderSectionAsync("Scripts", required: false)
    <script type="text/javascript">
        history.pushState(null, null, location.href);
        window.onpopstate = function() {
            history.go(1);
        };
        $(document).ready(function() {

        });
     

        $(function() {
            $('[data-toggle="tooltip"]').tooltip()
        })

        function msgError(mensaje) {
            toastr.error(mensaje, 'Validacion', {
                "positionClass": "toast-top-center",
                //"positionClass": "toast-top-full-width",
                timeOut: 5000,
                "closeButton": true,
                "debug": false,
                "newestOnTop": true,
                "progressBar": true,
                "preventDuplicates": true,
                "onclick": null,
                "showDuration": "300",
                "hideDuration": "1000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut",
                "tapToDismiss": false

            });
        }


    </script>
</body>
</html>
