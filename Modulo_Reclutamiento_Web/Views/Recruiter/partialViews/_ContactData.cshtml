@model ProspectusData

<div class="card">

    <div class="card-body">
        <div class="basic-form">

            <div class="form-group">
                <label asp-for="@Model.ContactData.PersonalEmail" class="text-muted m-b-15 f-s-12"></label>
                <input type="email" asp-for="@Model.ContactData.PersonalEmail" class="form-control input-rounded" placeholder="">
            </div>
            <div class="form-group">
                <label asp-for="@Model.ContactData.Phone" class="text-muted m-b-15 f-s-12"></label>
                <input type="number" asp-for="@Model.ContactData.Phone" class="form-control input-rounded" placeholder="">
            </div>
            <div class="form-group">
                <label asp-for="@Model.ContactData.MobilePhone" class="text-muted m-b-15 f-s-12"></label>
                <input type="number" asp-for="@Model.ContactData.MobilePhone" class="form-control input-rounded" placeholder="">
            </div>

        </div>
    </div>
</div>



