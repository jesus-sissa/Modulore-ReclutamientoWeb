@model ProspectusData
<div class="card">
    <div class="card-body">
        <div class="basic-form">

            <div class="form-group">
                <label asp-for="@Model.InfonaCredit.CreditInfonavit" class="text-muted m-b-15 f-s-12"></label>
                <select id="credInfOp" class="form-control input-rounded" asp-for="@Model.InfonaCredit.CreditInfonavit" asp-items="@(new SelectList(Model.InfonaCredit.CreditInfonavitOp.OrderBy(x=>x.Text).ToList(), "Value","Text",Model.InfonaCredit.CreditInfonavit))"></select>
            </div>
            <div class="form-group">
                <label asp-for="@Model.InfonaCredit.CreditNumber" class="text-muted m-b-15 f-s-12"></label>
                <input id="credInfCredNum" type="text" asp-for="@Model.InfonaCredit.CreditNumber" class="form-control input-rounded "  placeholder="">
            </div>
             <div class="form-group">
                <label asp-for="@Model.InfonaCredit.DiscountType" class="text-muted m-b-15 f-s-12"></label>
                <select id="credInfDisTyp" class="form-control input-rounded" disabled asp-for="@Model.InfonaCredit.DiscountType" asp-items="@(new SelectList(Model.InfonaCredit.DiscountTypeOp, "Value","Text",Model.InfonaCredit.DiscountTypeOp))"></select>
            </div>
             <div class="form-group">
                <label asp-for="@Model.InfonaCredit.DiscountedAmount" class="text-muted m-b-15 f-s-12"></label>
                <input id="credInfDisAm" type="text" disabled asp-for="@Model.InfonaCredit.DiscountedAmount" class="form-control input-rounded " placeholder="">
            </div>

        </div>
    </div>
</div>